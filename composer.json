{
    "name": "muscobytes/takeads-api-client",
    "version": "0.0.1",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Muscobytes\\TakeAdsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muscobytes\\TakeAdsApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^11.4@dev",
        "php-http/mock-client": "1.x-dev",
        "nyholm/psr7": "^1.8@dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": ">=8.3",
        "psr/http-client": "^1.0@dev",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.1",
        "psr/http-factory-implementation": "*"
    }
}
