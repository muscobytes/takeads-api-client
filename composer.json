{
    "name": "muscobytes/takeads-api-client",
    "version": "1.0.3",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Muscobytes\\TakeadsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muscobytes\\TakeadsApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^11.4@dev",
        "php-http/mock-client": "^1.6",
        "nyholm/psr7": "^1.8@dev",
        "symfony/http-client": "7.2.x-dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "require": {
        "php": ">=8.3",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*"
    }
}
